{
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "globals": {
    "expect": true,
    "sinon": true
  },
  "rules": {
    "newline-per-chained-call": [
      "error"
    ],
    "no-restricted-globals": [
      "error",
      "event",
      "fdescribe"
    ],
    "lines-between-class-members": [
      "error",
      "always"
    ],
    "no-multiple-empty-lines": "error",
    "no-console": "off",
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double"
    ]
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}