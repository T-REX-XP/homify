{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      true
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "no-shadowed-variable": false,
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
  },
  "rulesDirectory": []
}